#include <stdio.h>

int main()
{
    char word [100000]; 
    printf("Input any word:\n");
    scanf( "%s", &word);
    printf("Your word is: %s\n", word);
    int i = atoi(word); //atoi here only reads numbers. It does not read words. keep in mind for future reference
    printf("The value entered is: %d", i);

    return 0;
}



#include <stdio.h>

int main()
{
    char word [100000]; 
    int YN;
    printf("Input any word:\n");
    scanf( "%s", &word);
    printf("Your word is: %s\n", word);
    printf("Is that correct: Yes= 1, N0= 0?\n");
    scanf("%d", &YN);
   // printf("You entered %d\n", YN);
    if (YN > 0)
    {
        printf("You entered Yes\n");
    }
    else 
    {
        printf("You entered No\n");    
    }

    return 0;
}
