#include <stdio.h>


int main()
{
    float earning1, percentage1, save_1, hun_1 = 100.0;
    printf("SAVINGS PER MONTH\n");
    printf("Enter your monthly earnings: $");
    scanf("%f", &earning1);
    printf("Enter you percentage savings: ");
    scanf("%f", &percentage1);
    save_1 = (percentage1 * earning1) / hun_1;
    printf("Save: $%f", save_1);
    
    

    return 0;
}
